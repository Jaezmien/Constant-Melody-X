		<!--[ WITH 4TH BUTTON ]-->
		<Z
			Type="Quad"
			InitCommand="diffuse,0,0,0,0.8;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y+55;zoomto,SCREEN_WIDTH,150"
			OffCommand="%TitleMenuOut"
		/>
		
		<Layer 
			File="@THEME:GetPath(2,'Melody','SimplyLove (doubleres)')" 
			InitCommand="%function(self)
				self:x(SCREEN_CENTER_X)
				self:y(SCREEN_CENTER_Y - 130)
				self:bob()

				self:zoom( CONSTMELODY.MinimumVersion('V2') and 1 or 0.5 )
				self:effectmagnitude(0,20,0)
				self:effectperiod(4)
			end" 
			OffCommand="%TitleMenuOut"
		/>
		<Layer
			Font="_wendy white" Text="TIME: 69:69:69 PM"
			Command="horizalign,left;vertalign,middle;x,5;y,SCREEN_CENTER_Y-35;zoom,0.25;shadowlength,1;queuecommand,SetTime"
			SetTimeCommand="%function(self)
				local hour = Hour()
				local htyp = 'AM'
				if hour > 12 then hour = hour - 12; htyp = 'PM'; end -- 12 HOURS SYSTEM FTW >:VVV
				if hour == 0 then hour = 12 end
				--self:settext('TIME: '..hour..':'..Minute()..':'..Second()..' '..htyp)
				self:settext(string.format('TIME: %02d:%02d:%02d '..htyp,hour,Minute(),Second()))
				self:sleep(1/60)
				self:queuecommand('SetTime')
			end" OffCommand="%TitleMenuOut"
		/>
		<Layer Font="_equestria" Text="" Command="x,SCREEN_CENTER_X-140;y,SCREEN_CENTER_Y+140;zoom,0.6;shadowlength,1;diffusealpha,0;sleep,.3;queuecommand,Set" SetCommand="%function(self) s = SCREENMAN:GetTopScreen():GetChild('Songs'):GetText() self:settext(s) self:linear(.5) self:diffusealpha(1) Command = {} function file() if children then File=children[math.mod(table.getn(Command),table.getn(children))+1] end end end" OffCommand="%TitleMenuOut" />

		<Layer Font="_equestria" Text="" Command="x,SCREEN_CENTER_X+140;y,SCREEN_CENTER_Y+140;zoom,0.6;shadowlength,1;diffusealpha,0;sleep,.3;queuecommand,Set"
		SetCommand="%function(self) 
			local m_crashes = GetNumCrashLogs()
			if m_crashes then self:settext('Current Errors: '..m_crashes)
			else self:settext('Current Errors: None') end
			self:linear(0.5)
			self:diffusealpha(1)
		end" OffCommand="%TitleMenuOut" />
