<ActorFrame>
	<children>
		<AutoActor File="../_fade in" />
		<ActorFrame Condition="OnePassed()">
			<children>
				<AutoActor File="cleared text" Command="zoom,0.8;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y+50;diffusealpha,0;accelerate,0.4;diffusealpha,1;sleep,1.3;decelerate,0.4;diffusealpha,0" />
				</children> 
		</ActorFrame>
		<ActorFrame Condition="not OnePassed()">
			<children>
				<AutoActor File="failed text" Command="zoom,1;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y+50;diffusealpha,0;accelerate,0.4;diffusealpha,1;sleep,1.3;decelerate,0.4;diffusealpha,0" />
				</children>
		</ActorFrame>
	</children>
</ActorFrame>
